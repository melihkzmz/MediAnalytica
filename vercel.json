{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": "Skin-Disease-Classifier",
  "rewrites": [
    {
      "source": "/",
      "destination": "/Skin-Disease-Classifier/analyze.html"
    },
    {
      "source": "/(.*)",
      "destination": "/Skin-Disease-Classifier/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

